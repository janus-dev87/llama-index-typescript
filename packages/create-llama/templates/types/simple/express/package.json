{
  "name": "llama-index-express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup index.ts --format esm --dts",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsup index.ts --format esm --dts --watch\" \"nodemon -q dist/index.js\""
  },
  "dependencies": {
    "express": "^4",
    "llamaindex": "0.0.31"
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/node": "^20",
    "concurrently": "^8",
    "eslint": "^8",
    "nodemon": "^3",
    "tsup": "^7",
    "typescript": "^5"
  }
}